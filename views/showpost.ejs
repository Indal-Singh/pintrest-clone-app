            <a class="username"><%=user.username%></a>
            
            <div class="hover-zone">
              <div class="bottom-bar">
                <a href="<%= post.redirecturl ? post.redirecturl : '' %>">â†— <%= post.redirecturl ? new URL(post.redirecturl).hostname : '' %></a>
